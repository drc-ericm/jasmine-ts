{
  "name": "jasmine-ts",
  "version": "0.3.0",
  "description": "Execute jasmine with ts-node",
  "main": "lib/index.js",
  "bin": "cli.js",
  "types": "dist/index.d.ts",
  "files": ["lib/","cli.js"],
  "scripts": {
    "build": "npm run compile",
    "precompile": "rimraf lib",
    "compile": "tsc --project tsconfig.json",
    "lint": "./node_modules/.bin/tslint -p tsconfig.json",
    "jasmineTs": "node lib/index.js --config=spec/jasmine.json spec/jasmine-ts.spec.ts --transpileOnly --compilerOptions='{\"allowJs\": true}'",
    "test": "npm run lint && npm run compile && npm run jasmineTs",
    "prepublishOnly": "tsc",
    "prepublish": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svi3c/jasmine-ts.git"
  },
  "keywords": [
    "node",
    "testing",
    "tests",
    "jasmine",
    "typescript"
  ],
  "author": "Sven Reglitzki <sreglitzki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svi3c/jasmine-ts/issues"
  },
  "homepage": "https://github.com/svi3c/jasmine-ts#readme",
  "dependencies": {
    "tsconfig-paths": "^3.9.0",
    "yargs": "^13.2.4"
  },
  "peerDependencies": {
    "jasmine": ">=3.4",
    "ts-node": ">=3.2.0 <=8",
    "typescript": ">=3.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "@types/node": "^12.0.8",
    "@types/yargs": "^13.0.0",
    "jasmine": ">=3.4",
    "jasmine-spec-reporter": "^4.2.1",
    "rimraf": "^2.6.3",
    "ts-node": ">=3.2.0 <=8",
    "tslint": "^5.17.0",
    "typescript": ">=3.5.2"
  },
  "engines": {
    "node": ">= 5.12"
  }
}
